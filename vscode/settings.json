{
    "telemetry.enableCrashReporter": false,
    "telemetry.enableTelemetry": false,
    "editor.fontFamily": "Iosevka Nerd Font",
    "editor.fontSize": 14,
    "terminal.integrated.fontSize": 14,
    "terminal.integrated.fontFamily": "VictorMono Nerd Font",
    "editor.fontLigatures": true,
    "workbench.colorTheme": "spaceduck",
    "python.defaultInterpreterPath": "/usr/local/opt/python@3.9/libexec/bin/python",
    "customizeUI.titleBar": "frameless",
    "customizeUI.activityBar": "bottom",
    "launch": {


        "configurations": [],
        "compounds": []
    },
    "workbench.editorAssociations": {
        "*.ipynb": "jupyter-notebook"
    },
    "editor.suggestSelection": "first",
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
    "editor.wordWrap": "bounded",
    "python.languageServer": "Pylance",
    "workbench.activityBar.visible": false,
    "autoHide.autoHidePanel": false,
    "files.autoSave": "afterDelay",
    "customizeUI.font.monospace": "VictorMono Nerd Font",
    "customizeUI.font.regular": "VictorMono Nerd Font",
    "vim.useSystemClipboard": true,
    "focus.highlightRange": "fixed",
    "vim.statusBarColorControl": false,
    "vim.statusBarColors.normal": ["#8FBCBB", "#434C5E"],
    "vim.statusBarColors.insert": "#BF616A",
    "vim.statusBarColors.visual": "#B48EAD",
    "vim.statusBarColors.visualline": "#B48EAD",
    "vim.statusBarColors.visualblock": "#A3BE8C",
    "vim.statusBarColors.replace": "#D08770",
    "vim.statusBarColors.commandlineinprogress": "#007ACC",
    "vim.statusBarColors.searchinprogressmode": "#007ACC",
    "vim.statusBarColors.easymotionmode": "#007ACC",
    "vim.statusBarColors.easymotioninputmode": "#007ACC",
    "vim.statusBarColors.surroundinputmode": "#007ACC",
    "colorize.languages": [
        "css",
        "sass",
        "scss",
        "less",
        "postcss",
        "sss",
        "stylus",
        "xml",
        "svg",
        "shellscript"
    ],
    "[yaml]": {

        "editor.insertSpaces": true,
        "editor.tabSize": 2,
        "editor.autoIndent": "advanced"
    },
    "window.titleBarStyle": "native",
    "terminal.integrated.rendererType": "dom",
    "vscode_vibrancy.imports": [
    

        "$theme-style$"
    ],
    "vscode_vibrancy.opacity": 1,
    "terminal.explorerKind": "external",
    "terminal.external.osxExec": "iTerm2 x86.app",
    "debug.allowBreakpointsEverywhere": true,
    "debug.console.fontSize": 14,
    "prettier.documentSelectors": [
        "**/*.{ts,js,py}"
    ],
    "[javascript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "workbench.sideBar.location": "right",
    "editor.wordWrapColumn": 120,
    "vscode-neovim.neovimInitVimPaths.darwin": "~/.config/nvim/init.vim",
    "vscode-neovim.neovimExecutablePaths.darwin": "/usr/local/bin/nvim",
    "vscode-neovim.mouseSelectionStartVisualMode": true,
    "todohighlight.include": [
        "**/*.js",
        "**/*.jsx",
        "**/*.ts",
        "**/*.tsx",
        "**/*.html",
        "**/*.css",
        "**/*.scss",
        "**/*.php",
        "**/*.rb",
        "**/*.txt",
        "**/*.mdown",
        "**/*.md",
        "**/*.py"
    ],
    "todohighlight.keywords": [
        "TODO:",
        "XXX:",
        "HACK:",
        "NOTE:",
        "FIXME:"
    ],
    "workbench.statusBar.visible": false,
    "python.formatting.provider": "black",
    "editor.formatOnSave": true,
    "terminal.integrated.gpuAcceleration": "no",
    "notebook.cellToolbarLocation": {
        "default": "right",
        "jupyter-notebook": "left"
    },
    "bracketPairColorizer.independentPairColors": [
        [
        "Gold",
        "Orchid",
        "LightSkyBlue",
        "Red"
        ],
    ],
    "editor.inlayHints.fontFamily": "Iosevka Nerd Font",
    "editor.fontWeight": semibold,
    "editor.formatOnPaste": trueki 

}