/opt/homebrew/bin/brew services stop spacebar;\
killall limelight;\
/usr/bin/find /usr/bin/find /Volumes/Wallpapers/Organize -type f -print0 | /usr/local/opt/coreutils/libexec/gnubin/shuf -z -n 1| /usr/bin/xargs -I{} /usr/bin/osascript -e "tell application \"Finder\" to set desktop picture to POSIX file \"{}\"";\
current_wallpaper="$(osascript -e 'tell app "finder" to get posix path of (get desktop picture as alias)')";\
wal -i $current_wallpaper --backend wal  -n;\
osascript -e "tell application \"Alfred 4\" to set theme \"Wal\"";\
/usr/local/bin/pywalfox update;\
/opt/homebrew/bin/brew services start spacebar;\
/Users/brandon/.config/bin/limelight;\
sh $HOME/.config/spacebar/spacebarrc;\
sh $HOME/.config/limelight/limelightrc;